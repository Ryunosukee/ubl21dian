        "source": "https://github.com/torresoftware/ubl21dian"
    },
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "facturalatam",
            "email": "soporte@facturalatam.com",
            "homepage": "https://facturalatam.com"
        }
    ],
    "minimum-stability": "stable",
